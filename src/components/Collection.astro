---
const { title, img_url } = Astro.props;
---

<div class="container">
  <div class="row d-flex">
    <div class="col-12 col-md-6 col-lg-4">
      <img src={img_url} alt="rings" class="w-100 image" />
      <p class="fs-3 mt-2">
        <a href="" class="text-black">{title}</a>
      </p>
    </div>
  </div>
</div>
